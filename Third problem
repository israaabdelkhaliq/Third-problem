#include<iostream>
#include <string>
using namespace std;
 int main()
{
    cout<<" please enter any string with backslash\n  ";
	string string1 ;
	getline(cin,string1);
	int i=0;
	while(i< string1.size() && string1[i]!='\\'){
		cout<<string1[i];
		++i;
	}
	
}